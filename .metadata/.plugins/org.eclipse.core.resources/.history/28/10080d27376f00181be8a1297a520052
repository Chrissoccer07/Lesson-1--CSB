
public class BigPapi {

	public static void main(String args[]) {
		double x = 6;
		
		double y = 5;
		
		double z = x/y;
		
		System.out.println(y * x);
		System.out.println(x+y+z*z/x+y-x+ " Kittens");
		System.out.println(y * x);
		System.out.println(y * x);
		System.out.println(y * x);
		
		Puppy ourpuppy = new Puppy("TIM");
		
		ourpuppy.setAge(5);
		ourpuppy.getAge();
		
		System.out.println("our variable -" + ourpuppy.puppyAge);
	}
}
